{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Process",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "apiVersion": 1,
            "port": 2345
        },
        {
            "name": "Launch in external terminal",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "console": "externalTerminal",
            "debugAdapter": "legacy"
        }
    ]
}